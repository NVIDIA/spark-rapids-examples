    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"DEFAULT_TOOLS_VER = \\\"24.12.4\\\"\\n\",\n",
    "    \"TOOLS_VER_ARG = dbutils.widgets.get(\\\"Tools Version\\\")\\n\",\n",
    "    \"TOOLS_VER = TOOLS_VER_ARG if TOOLS_VER_ARG else DEFAULT_TOOLS_VER\\n\",\n",
    "    \"print(f\\\"Using Tools Version: {TOOLS_VER}\\\")\"\n",
    "   ]\n",
